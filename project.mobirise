          "_name": "form1",
          "_customHTML": "<section class=\"form1\" group=\"Forms\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" value=\"0\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMaintitle\" checked=\"\">\n        <header>Colors</header>\n        <input type=\"color\" title=\"Background\" name=\"bgc\" checked=\"\" value=\"#0a1c44\">\n        <input type=\"color\" title=\"Line\" name=\"lineColor\" value=\"#ffffff\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" value=\"../_images/background2.jpg\" title=\"Image\" selected=\"\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Color\">\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=9morbKMwCu4\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Color\" name=\"overlayColor\" value=\"#656565\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container-fluid\">\n        <div class=\"row justify-content-center\">\n\n            <div class=\"col-lg-6 col-md-5 col-sm-12 align-center\">\n\n\n            </div>\n\n\n\n\n            <div class=\"col-lg-6 col-md-7 col-sm-12 align-left card\">\n                <h2 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style align-left\" mbr-theme-style=\"display-2\" mbr-if=\"showMaintitle\" data-app-selector=\".mbr-section-title, .line-wrap\">Fill in the Form</h2>\n                <div class=\"line-wrap\" mbr-if=\"showMaintitle\">\n                    <div class=\"line\"></div>\n                </div>\n                <div mbr-form=\"\">\n                    <!---Formbuilder Form--->\n                    <form action=\"\" name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                        <div class=\"row\">\n                            <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">\n                            </div>\n                            </div>\n                        <div class=\"dragArea row\">\n                            <div class=\"col-md-12  form-group\" data-for=\"name\">\n                                <label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"name\" class=\"form-control-label mbr-fonts-style\">First Name</label>\n                                <input type=\"text\" name=\"name\" placeholder=\"Your First Name\" data-form-field=\"Name\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\">\n                            </div>\n                            <div data-for=\"lastname\" class=\"col-md-12  form-group\">\n                                <label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"lastname\" class=\"form-control-label mbr-fonts-style\">Last Name</label>\n                                <input type=\"text\" name=\"lastname\" placeholder=\"Your Last Name\" data-form-field=\"Last Name\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\">\n                            </div>\n                            <div data-for=\"phone\" class=\"col-md-12  form-group\">\n                                <label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"phone\" class=\"form-control-label mbr-fonts-style\">Phone</label>\n                                <input type=\"tel\" name=\"phone\" placeholder=\"Your Phone Number\" data-form-field=\"Phone\" mbr-theme-style=\"display-7\" class=\"form-control\">\n                            </div>\n                            <div data-for=\"mail\" class=\"col-md-12  form-group\">\n                                <label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"mail\" class=\"form-control-label mbr-fonts-style\">E-mail</label>\n                                <input type=\"email\" name=\"mail\" placeholder=\"E-mail\" data-form-field=\"E-mail\" mbr-theme-style=\"display-7\" class=\"form-control\">\n                            </div>\n                            <div data-for=\"department\" class=\"col-md-12  form-group\">\n                                <label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"department\" class=\"form-control-label mbr-fonts-style\">Department</label>\n                                <input type=\"text\" name=\"department\" placeholder=\"Your Department\" data-form-field=\"Department\" mbr-theme-style=\"display-7\" class=\"form-control\">\n                            </div>\n                            <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\" class=\"col-md-12  input-group-btn\">\n                                <a type=\"submit\" class=\"btn btn-form btn-lg btn-primary\">Send Message</a>\n                            </div>\n                        </div>\n                    </form><!---Formbuilder Form--->\n                </div>\n            </div>